    %pushi/vec4 200, 0, 32;
T_4.19 %dup/vec4;
    %cmpi/s 0, 0, 32;
    %jmp/1xz T_4.20, 5;
    %jmp/1 T_4.20, 4;
    %subi 1, 0, 32;
    %wait E_0x5555682367c0;
    %jmp T_4.19;
T_4.20 ;
    %pop/vec4 1;
    %vpi_call/w 3 108 "$display", "\012Test 5: Final positions" {0 0 0};
    %vpi_call/w 3 109 "$display", "Ball at (%0d, %0d)", v0x55556826ccc0_0, v0x55556826cda0_0 {0 0 0};
    %vpi_call/w 3 110 "$display", "Paddle at (%0d, %0d)", v0x55556826d140_0, v0x55556826d210_0 {0 0 0};
    %load/vec4 v0x55556826ccc0_0;
    %pad/u 32;
    %cmpi/u 8, 0, 32;
    %flag_inv 5; GE is !LT
    %flag_get/vec4 5;
    %jmp/0 T_4.23, 5;
    %load/vec4 v0x55556826ccc0_0;
    %pad/u 32;
    %cmpi/u 632, 0, 32;
    %flag_get/vec4 5;
    %and;
T_4.23;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.21, 8;
    %jmp T_4.22;
T_4.21 ;
    %vpi_call/w 3 113 "$error", "Ball X out of bounds" {0 0 0};
T_4.22 ;
    %load/vec4 v0x55556826cda0_0;
    %pad/u 32;
    %cmpi/u 8, 0, 32;
    %flag_inv 5; GE is !LT
    %flag_get/vec4 5;
    %jmp/0 T_4.26, 5;
    %load/vec4 v0x55556826cda0_0;
    %pad/u 32;
    %cmpi/u 472, 0, 32;
    %flag_get/vec4 5;
    %and;
T_4.26;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.24, 8;
    %jmp T_4.25;
T_4.24 ;
    %vpi_call/w 3 114 "$error", "Ball Y out of bounds" {0 0 0};
T_4.25 ;
    %delay 1000000, 0;
    %vpi_call/w 3 117 "$display", "\012All tests completed successfully!" {0 0 0};
    %vpi_call/w 3 118 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x555568234b80;
T_5 ;
    %wait E_0x555568212220;
    %load/vec4 v0x55556826d0a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.0, 8;
    %load/vec4 v0x55556826cf40_0;
    %pad/s 32;
    %load/vec4 v0x55556826d2e0_0;
    %load/vec4 v0x55556826ccc0_0;
    %sub;
    %pad/s 32;
    %mul;
    %cmpi/s 0, 0, 32;
    %jmp/0xz  T_5.2, 5;
    %vpi_call/w 3 126 "$display", "Time %0t: Horizontal bounce detected at (%0d, %0d)", $time, v0x55556826ccc0_0, v0x55556826cda0_0 {0 0 0};
T_5.2 ;
