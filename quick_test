    %load/vec4 v0x55558be15240_0;
    %pad/u 32;
    %cmpi/u 240, 0, 32;
    %jmp/0xz  T_4.20, 5;
    %vpi_call/w 3 95 "$display", "  ERROR: Ball moving up instead of down" {0 0 0};
    %vpi_call/w 3 96 "$finish" {0 0 0};
T_4.20 ;
    %vpi_call/w 3 98 "$display", "  PASS: Ball is moving right and down\012" {0 0 0};
    %vpi_call/w 3 101 "$display", "Test 3: Continuous Movement" {0 0 0};
    %fork t_1, S_0x55558bdf4c50;
    %jmp t_0;
    .scope S_0x55558bdf4c50;
t_1 ;
    %load/vec4 v0x55558be15160_0;
    %store/vec4 v0x55558bdc5110_0, 0, 10;
    %load/vec4 v0x55558be15240_0;
    %store/vec4 v0x55558bdc5440_0, 0, 10;
    %pushi/vec4 20, 0, 32;
T_4.22 %dup/vec4;
    %cmpi/s 0, 0, 32;
    %jmp/1xz T_4.23, 5;
    %jmp/1 T_4.23, 4;
    %subi 1, 0, 32;
    %wait E_0x55558bddf060;
    %jmp T_4.22;
T_4.23 ;
    %pop/vec4 1;
    %wait E_0x55558bdda8b0;
    %vpi_call/w 3 109 "$display", "  After 20 more frames:" {0 0 0};
    %vpi_call/w 3 110 "$display", "    Ball moved from (%0d,%0d) to (%0d,%0d)", v0x55558bdc5110_0, v0x55558bdc5440_0, v0x55558be15160_0, v0x55558be15240_0 {0 0 0};
    %load/vec4 v0x55558be15160_0;
    %load/vec4 v0x55558bdc5110_0;
    %sub;
    %load/vec4 v0x55558be15240_0;
    %load/vec4 v0x55558bdc5440_0;
    %sub;
    %vpi_call/w 3 111 "$display", "    Delta: (%0d, %0d)", S<1,vec4,s10>, S<0,vec4,s10> {2 0 0};
    %load/vec4 v0x55558be15160_0;
    %load/vec4 v0x55558bdc5110_0;
    %cmp/e;
    %flag_get/vec4 4;
    %jmp/0 T_4.26, 4;
    %load/vec4 v0x55558be15240_0;
    %load/vec4 v0x55558bdc5440_0;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_4.26;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.24, 8;
    %vpi_call/w 3 114 "$display", "  ERROR: Ball stopped moving" {0 0 0};
    %vpi_call/w 3 115 "$finish" {0 0 0};
T_4.24 ;
    %vpi_call/w 3 117 "$display", "  PASS: Ball continues to move\012" {0 0 0};
    %end;
    .scope S_0x55558bdd90e0;
t_0 %join;
    %vpi_call/w 3 121 "$display", "Test 4: Paddle Tracking" {0 0 0};
    %load/vec4 v0x55558be15580_0;
    %pad/u 32;
    %addi 32, 0, 32;
    %vpi_call/w 3 122 "$display", "  Ball Y: %0d, Paddle Y: %0d, Paddle center: %0d", v0x55558be15240_0, v0x55558be15580_0, S<0,vec4,u32> {1 0 0};
    %load/vec4 v0x55558be15580_0;
    %pad/u 32;
    %addi 32, 0, 32;
    %load/vec4 v0x55558be15240_0;
    %pad/u 32;
    %subi 20, 0, 32;
    %cmp/u;
    %jmp/1 T_4.29, 5;
    %flag_mov 8, 5;
    %load/vec4 v0x55558be15240_0;
    %pad/u 32;
    %addi 20, 0, 32;
    %load/vec4 v0x55558be15580_0;
    %pad/u 32;
    %addi 32, 0, 32;
    %cmp/u;
    %flag_or 5, 8;
T_4.29;
    %jmp/0xz  T_4.27, 5;
    %vpi_call/w 3 127 "$display", "  WARNING: Paddle not tracking ball well (may need more time)" {0 0 0};
    %jmp T_4.28;
T_4.27 ;
    %vpi_call/w 3 129 "$display", "  PASS: Paddle tracking ball" {0 0 0};
T_4.28 ;
    %vpi_call/w 3 132 "$display", "\012=== ALL TESTS PASSED ===" {0 0 0};
    %vpi_call/w 3 133 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x55558bdd90e0;
T_5 ;
