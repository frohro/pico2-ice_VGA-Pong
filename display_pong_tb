    %jmp/0 T_13.26, 4;
    %load/vec4 v0x55555838e310_0;
    %load/vec4 v0x55555838fc80_0;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_13.26;
    %flag_set/vec4 8;
    %jmp/0xz  T_13.24, 8;
    %vpi_call/w 3 144 "$error", "Ball is not moving!" {0 0 0};
