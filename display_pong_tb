    %jmp T_13.19;
T_13.18 ;
    %load/vec4 v0x55555838e310_0;
    %load/vec4 v0x55555838fc80_0;
    %sub;
    %vpi_call/w 3 122 "$display", "  Ball Y moved by %0d pixels", S<0,vec4,u10> {1 0 0};
T_13.19 ;
    %vpi_call/w 3 125 "$display", "\012Test 3: Verify continuous movement over 20 frames" {0 0 0};
    %load/vec4 v0x55555838e200_0;
    %store/vec4 v0x55555838fb80_0, 0, 10;
    %load/vec4 v0x55555838e310_0;
    %store/vec4 v0x55555838fc80_0, 0, 10;
    %pushi/vec4 20, 0, 32;
T_13.20 %dup/vec4;
    %cmpi/s 0, 0, 32;
    %jmp/1xz T_13.21, 5;
    %jmp/1 T_13.21, 4;
    %subi 1, 0, 32;
    %wait E_0x555558326b60;
    %wait E_0x5555583255a0;
