    %vpi_call/w 3 109 "$display", "  Previous - Ball: (%0d, %0d), Paddle Y: %0d", v0x55555838fb80_0, v0x55555838fc80_0, v0x555558390260_0 {0 0 0};
    %vpi_call/w 3 111 "$display", "  Current  - Ball: (%0d, %0d), Paddle Y: %0d", v0x55555838e200_0, v0x55555838e310_0, v0x55555838ee00_0 {0 0 0};
    %load/vec4 v0x55555838e200_0;
    %load/vec4 v0x55555838fb80_0;
    %cmp/e;
    %jmp/0xz  T_13.16, 4;
    %vpi_call/w 3 115 "$error", "Ball X did not move! Still at %0d", v0x55555838e200_0 {0 0 0};
    %jmp T_13.17;
T_13.16 ;
    %load/vec4 v0x55555838e200_0;
    %load/vec4 v0x55555838fb80_0;
