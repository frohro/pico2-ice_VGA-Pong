    %vpi_call/w 3 136 "$display", "After 20 more frames:" {0 0 0};
    %vpi_call/w 3 137 "$display", "  Ball moved from (%0d, %0d) to (%0d, %0d)", v0x55555838fb80_0, v0x55555838fc80_0, v0x55555838e200_0, v0x55555838e310_0 {0 0 0};
    %load/vec4 v0x55555838e200_0;
    %load/vec4 v0x55555838fb80_0;
    %sub;
