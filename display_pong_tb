T_13.25 ;
    %vpi_call/w 3 149 "$display", "\012Test 4: Check frame_start pulse characteristics" {0 0 0};
    %fork t_1, S_0x555558336ff0;
    %jmp t_0;
    .scope S_0x555558336ff0;
t_1 ;
    %wait E_0x555558326b60;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x555558359ce0_0, 0, 32;
T_13.27 ;
    %load/vec4 v0x55555838e780_0;
    %flag_set/vec4 8;
    %jmp/0xz T_13.28, 8;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x555558359ce0_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x555558359ce0_0, 0, 32;
    %wait E_0x5555583255a0;
    %jmp T_13.27;
T_13.28 ;
    %vpi_call/w 3 159 "$display", "frame_start pulse width: %0d cycles", v0x555558359ce0_0 {0 0 0};
    %load/vec4 v0x555558359ce0_0;
    %cmpi/ne 1, 0, 32;
    %jmp/0xz  T_13.29, 4;
    %vpi_call/w 3 161 "$error", "frame_start should be single cycle pulse, got %0d cycles", v0x555558359ce0_0 {0 0 0};
    %jmp T_13.30;
T_13.29 ;
    %vpi_call/w 3 163 "$display", "frame_start pulse is correct (1 cycle)" {0 0 0};
T_13.30 ;
    %end;
    .scope S_0x555558336e60;
t_0 %join;
    %delay 10000000, 0;
