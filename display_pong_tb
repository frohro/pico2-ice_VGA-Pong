    %vpi_call/w 3 117 "$display", "  Ball X moved by %0d pixels", S<0,vec4,u10> {1 0 0};
T_13.17 ;
    %load/vec4 v0x55555838e310_0;
    %load/vec4 v0x55555838fc80_0;
    %cmp/e;
